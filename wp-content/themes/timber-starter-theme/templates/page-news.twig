{% extends "base.twig" %}

{% block content %}
	<div class="content-wrapper">
		<article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
			<section class="article-content white-bg">
        <div class="article-body">
          {{post.content}}
				</div>
        <h1 class="article-h1">{{post.title}}</h1>
        {{news}}
			</section>
		</article>
	</div><!-- /content-wrapper -->
{% endblock %}
